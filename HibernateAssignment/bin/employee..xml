<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.assignment.Employee" table="employee12">
<id name="name" column="sname"></id>
<property name="dateofbirth" column="dateofbirth"></property>
<property name="department" column="department"></property>
<property name="designation" column="designation"></property>
<property name="salary" column="salary"></property>
</class>
</hibernate-mapping>